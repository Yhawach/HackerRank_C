#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	
    int a,b,sum=0,sub=0;
    scanf("%d%d",&a,&b);
    sum=a+b;
    sub=a-b;
    printf("%d %d \n",sum,sub);
    float x,y,s=0,sb=0;
    scanf("%f %f",&x,&y);
    s=x+y;
    sb=x-y;
    printf("%0.1f %0.1f",s,sb);
    return 0;
}
